
public class Hello {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Hello World!");
		
		int a = 5;				//별 및 공백을 구분하기 위한 변수선언
		for(int i=1;i<=5 ;i++){
			for(int j=1;j<=5;j++){
				if(j>=a){
					System.out.print("*");
				}else{
					System.out.print(" ");
				}
			}
			a--;
			System.out.println();
		}
	}

}
