//컴퓨터학과, 20201704, 이현아
public class Practice15 {

	public static void main(String[] args) {
		int count = 0;
		
		for(int i=1; i<= 100; i += 3) {
			System.out.print(i + "\t");
			
			count++;
						
			if(count % 5 == 0) {
				System.out.print("\n");
			}
		}
		System.out.println("\n");
		
		int j = 0;
		count = 0;
		while(j <= 100) {
			System.out.print(j + "\t");
			
			count++;
			if(count % 5 == 0) {
				System.out.print("\n");
			
			j += 3;			
		}
		

	}

}
