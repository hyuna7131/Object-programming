//컴퓨터학과, 20201704, 이현아
public class Practice64 {

	public static void main(String[] args) {
		Dice dice1 = new Dice();
		Dice dice2 = new Dice();

		for(int i=1; dice1.getValue() == dice2.getValue(); i++) {
			System.out.print(i+"차 : 첫번째 주사위 = " + dice1.getValue());
			System.out.println(", 두번째 주사위 = " + dice2.getValue());
		}
		System.out.print("게임이 종료되었습니다.");
	}

}
